<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <LinearLayout
        android:id="@+id/license_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="26dp"
        android:paddingBottom="10dp"
        android:orientation="vertical">

        <com.kyad.traystorage.app.common.font.FontTextView
            style="@style/font_spoqahansansneo_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="2dp"
            android:text="1Password App Extension"
            android:includeFontPadding="false"
            android:textColor="@color/C151515"
            android:textSize="15dp" />

        <com.kyad.traystorage.app.common.font.FontTextView
            style="@style/font_spoqahansansneo_regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="https://github.com"
            android:includeFontPadding="false"
            android:textColor="@color/focus_color"
            android:textSize="15dp" />

        <com.kyad.traystorage.app.common.font.FontTextView
            style="@style/font_spoqahansansneo_regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lineHeight="20dp"
            android:includeFontPadding="false"
            android:text="Copyright 2017"
            android:textColor="@color/C151515"
            android:textSize="14dp"/>

    </LinearLayout>

</layout>